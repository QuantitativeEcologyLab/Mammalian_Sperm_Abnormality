@article{ WOS:000371049800034,
Author = {Kuhn, Jens H. and Lauck, Michael and Bailey, Adam L. and Shchetinin,
   Alexey M. and Vishnevskaya, Tatyana V. and Bao, Yiming and Ng, Terry Fei
   Fan and LeBreton, Matthew and Schneider, Bradley S. and Gillis, Amethyst
