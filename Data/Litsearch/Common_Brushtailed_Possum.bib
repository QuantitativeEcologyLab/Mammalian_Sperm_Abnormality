@article{ WOS:000277369100006,
Author = {DeGabriel, Jane L. and Moore, Ben D. and Marsh, Karen J. and Foley,
   William J.},
Title = {The effect of plant secondary metabolites on the interplay between the
   internal and external environments of marsupial folivores},
Journal = {CHEMOECOLOGY},
