@article{ WOS:000430158100008,
Author = {Witt, Ryan R. and Rodger, John C.},
Title = {Recent advances in tools and technologies for monitoring and controlling
   ovarian activity in marsupials},
Journal = {THERIOGENOLOGY},
Year = {2018},
Volume = {109},
Number = {SI},
Pages = {58-69},
Month = {MAR 15},
Abstract = {Components of assisted reproduction technologies (ART), such as sperm
   cryopreservation, artificial insemination, superovulation and pouch
   young surrogacy, have been developed for a range of Australian and
   American marsupials. However, methods to effectively control ovarian
   function, arguably the key limiting factors in applying and integrating
   ART as a practical tool in conservation management, remain poorly
   developed. This is largely due to unique characteristics of the
   marsupial corpus luteum and its failure to respond to agents used to
   synchronize ovarian function in eutherian mammals. This paper presents
   an overview of relevant aspects of marsupial reproductive biology across
   marsupial taxonomic groups including information on the long-established
   technique of removal of suckling young to activate ovarian cycles.
   Ovarian monitoring tools for marsupials are reviewed and their
   usefulness for ART assessed (laparotomy, hormone cycling, vaginal
   cytology, laparoscopy and ultrasonography). We also discuss promising
   recent work examining the potential of manipulating
   hypothalamic-pituitary function using GnRH agonists and antagonists as
   the basis of ovarian control (female synchronization) strategies. (C)
   2017 Elsevier Inc. All rights reserved.},
