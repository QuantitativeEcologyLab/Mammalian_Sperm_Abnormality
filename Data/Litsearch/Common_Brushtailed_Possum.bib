@article{ WOS:000084560900010,
Author = {Belov, K and Harrison, GA and Cooper, DW},
Title = {Cloning of the red kangaroo (<i>Macropus rufus</i>) follicle stimulating
   hormone beta subunit},
Journal = {REPRODUCTION FERTILITY AND DEVELOPMENT},
Year = {1998},
Volume = {10},
Number = {3},
Pages = {289-291},
Abstract = {The cDNA encoding the follicle stimulating hormone beta subunit
   (FSH-beta) was isolated from a red kangaroo pituitary cDNA library by
   using a porcine probe and the nucleotide sequence for the coding region
   was determined. The highest degree of deduced amino acid sequence
   identity (91\%) was observed between the red kangaroo and another
   marsupial, the brushtail possum (Trichosurus vulpecula), followed by
   eutherian species (76\%, 75\% and 74\%, respectively, for pig, mouse and
   sheep). Based on the deduced red kangaroo FSH-beta amino acid sequence,
   putative antigenic sites have been identified that may prove useful for
   studying the hormonal control of reproduction in marsupials.},
