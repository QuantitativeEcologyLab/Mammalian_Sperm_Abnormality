@article{ WOS:A1997YG68000013,
Author = {McFarlane, JR and Rudd, CD and Foulds, LM and Fletcher, TP and Renfree,
   MB},
Title = {Isolation and partial characterization of tammar wallaby luteinizing
   hormone and development of a radioimmunoassay},
